filetype plugin indent on
syntax on
silent! runtime macros/matchit.vim
set autochdir
set backspace=indent,eol,start
set hidden
set smartindent
set tabstop=4
set shiftwidth=4
set textwidth=120
set incsearch
set comments=sl:/*,mb:\ *,elx:\ */
set laststatus=2
set ruler
set switchbuf=useopen,usetab
set tags=./tags,tags;/
set wildmenu
set noerrorbells
set magic
set number
set hlsearch
set smartcase
set wildmenu
set noswapfile
set ai
